{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "jquery": true,
      "es6": true,
      "node": true
    },
    "globals": {
      "Rhino": true,
      "_": true,
      "React": true,
      "ReactDOM": true,
      "$": true,
      "wa": true,
      "_cuckoo": true,
      // native相关
      "Internal": true,
      "__bridge_callback": true,
      "__writeLocalStorage": true,
      // 地图API
      "google": true,
      "BMap": true,
      "ga": true,
      "_sendOrder": true,
      //
      // jasmine
      //
      "jasmineRequire": true,
      "jasmine": true,
      "describe": true,
      "it": true,
      "fdescribe": true,
      "fit": true,
      "expect": true,
      "beforeEach": true,
      "beforeAll": true,
      "afterEach": true,
      "afterAll": true,
      "spyOn": true
    },
    "rules": {
      "no-console": "off",
      "no-unsafe-negation": "error",
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
      "react/display-name": "off",
      "react/prop-types": "off",
      "react/no-deprecated": [
        "warn"
      ]
    },
    "plugins": [
      "react"
    ]
  }